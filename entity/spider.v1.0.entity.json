      },
      "textures": {
        "default": "textures/entity/spider/spider",
        "default2": "textures/entity/spider/spider_alpha",
        "default3": "textures/entity/spider/spider_alpha",
        "default4": "textures/entity/spider/spider_alpha",
        "default5": "textures/entity/spider/spider_alpha",
        "default6": "textures/entity/spider/spider_alpha",
        "default7": "textures/entity/spider/spider_alpha",
        "default8": "textures/entity/spider/spider_alpha",
        "default9": "textures/entity/spider/bone_spider",
        "default10": "textures/entity/spider/bone_spider",
        "default11": "textures/entity/spider/spider",
        "default12": "textures/entity/spider/spider",
        "default13": "textures/entity/spider/spider",
        "default14": "textures/entity/spider/spider",
        "default15": "textures/entity/spider/spider",
        "default16": "textures/entity/spider/spider",
        "default17": "textures/entity/spider/spider",
