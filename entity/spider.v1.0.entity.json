        "texture": "spawn_egg",
        "texture_index": 11
      },
      "scripts": {
        "pre_animation": [
          "variable.Nvar =(variable.Nvar ?? Math.random(-1,1));",
          "variable.Avar = (variable.Avar ?? Math.random(-20,20));",
          "variable.Xvar = (variable.Xvar ?? Math.random(0,999));"
        ]
      }
    }
  }
